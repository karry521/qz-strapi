{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "required": true,
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 8,
      "regex": "^(?=.*[a-z])(?=.*[A-Z]).{8,}$",
      "private": true,
      "searchable": false,
      "required": true,
      "configurable": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "private": true,
      "searchable": false,
      "configurable": false
    },
    "confirmationToken": {
      "type": "string",
      "private": true,
      "searchable": false,
      "configurable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "unique": true,
      "required": true
    },
    "phone": {
      "type": "string",
      "regex": "^1\\d{10}$",
      "required": false
    },
    "companyName": {
      "type": "string"
    },
    "userType": {
      "type": "enumeration",
      "enum": [
        "business",
        "individuals"
      ],
      "required": true,
      "default": "individuals"
    },
    "last_login_ip": {
      "type": "string"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "last_login_time": {
      "type": "datetime"
    },
    "basic_basic_limit": {
      "type": "integer",
      "required": false
    },
    "basic_extra_limit": {
      "type": "integer",
      "required": false
    },
    "basic_expire_time": {
      "type": "datetime"
    },
    "basic_priority": {
      "type": "integer",
      "required": true
    },
    "advanced_basic_limit": {
      "type": "integer",
      "required": true
    },
    "advanced_extra_limit": {
      "type": "integer",
      "required": true
    },
    "advanced_expire_time": {
      "type": "datetime"
    },
    "advanced_priority": {
      "type": "integer",
      "required": true
    },
    "devices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::device.device",
      "mappedBy": "user"
    }
  }
}